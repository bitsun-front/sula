{
  "version": 2,
  "public": false,
  "github": {
    "enable": false
  },
  "scope": "umijs",
  "name": "sula",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": ".doc" }
    }
